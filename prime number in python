def prime_num(num):
 flag =True
 if (num ==0 or num==1):
     print("Not a prime number")
 else :
     for i in range (2, num):
      if (num%i ==0):
         flag =True
         break
 return flag

print("Enter the number:")
num = int(input())
result = prime_num(num)
if result:
    print("Num" ,num, "is not a prime number")
else :
    print("Num" ,num, "is  a prime number")
