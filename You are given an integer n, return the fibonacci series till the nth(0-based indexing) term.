def fibonacci(a, b, n):
    print(a)
    if n == 1:
        return
    print(b)
    for _ in range(n - 2):
        c = a + b
        print(c)
        a, b = b, c

a, b, n = map(int, input("Enter a, b, n: ").split())
fibonacci(a, b, n)


or ===================================
def fibonacci(n):
    a, b = 0, 1
    for i in range(n):
        print(a)
        a, b = b, a + b

n = int(input("Enter n: "))
fibonacci(n)
