#include <stdio.h>
int n,i,j,count=0;;
int main()
{
    printf("enter the number:");
    scanf("%d", &n);
    for (i=1; i<n; i++){
         
        if (n%i==0){
            for (j=1; j<=i; j++){
                if ((i%j)==0){
                    count=count+1;
                }
                
                }
                if(count <3){
                printf("%d", i);
                    
                
                
                
            }
            count=0;
           
        }
    }

    return 0;
}
