#include <stdio.h>


    int sum =0,n,i,x;
int main() {
    // Write C code here
    printf("enter the number:");
    scanf("%d", &n);
    for(i=1; i<(n); i++){
        x =n%i;
        if (x==0){
            sum = sum +(i);
        }
    }
    if (n == sum){
        printf("%d is a perfect number", n);
    }
    else{
        printf("%d is not a perfect number",n);
    }
        
    

    return 0;
}
