def pallindrom(num):
    reverse_num=0
    while num !=0:
     digit =num%10
     reverse_num =reverse_num*10+digit
     num=num //10
    return reverse_num
print("Enter  the num:")
num =int(input())
reverse_num = pallindrom(num)
if (reverse_num == num):
    print("Number", num, "is a pallindrom")
else:
     print("Number", num, "is not a pallindrom")
