// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>

int main() {
     char str[20];
    char key = 'G';
    printf("enter the string:");
    scanf("%[^\n]s", str);
    printf("%s\n", str);
    int length;
    length = strlen(str);
    
    for (int i=0; i<length; i++){
        if (str[i]==key){
            continue;
        }
        else {
  
        printf ("%c", str[i]);
            
        }
        
    }
 
    return 0;
}
